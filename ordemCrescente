import java.util.Arrays;
import java.util.Scanner;


public class Q09 {

	public static void main(String[] args) {
		
		int n,j,contador=0,contMultiplos=0;
		double cont,i;
		
		 
		Scanner entrada = new Scanner(System.in);
		System.out.println("Digite um n√∫mero n : ");
		n = entrada.nextInt();
		
		System.out.println("Digite um numero i");
		i = entrada.nextInt();
		
		System.out.println("Digite um numero j");
		j = entrada.nextInt();
		
		while (contMultiplos < n) {
			
		      if (contador % i == 0 || contador % j ==0) {
		    	  
		    	  System.out.println(contador);
				  contMultiplos = contMultiplos + 1;
		}
		   	
			contador++;
		
			}
				System.out.println("quantidade de multiplos:" + contMultiplos);
		
			
			
	}
}
		
